<h1>会員一覧</h1>

<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
    <tr>
      <td><%= membr.id %></td>
      <td><%= membr.last_name %><%= member.first_name %></td>
      <td><%= membr.email %></td>
      <td><%= membr.is_deleted %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @members %>